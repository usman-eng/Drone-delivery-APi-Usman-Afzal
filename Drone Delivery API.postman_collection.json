						{
							"listen": "test",
							"script": {
								"exec": [
									"let response = pm.response.json();\r",
									"if (response.data.token) {\r",
									"    pm.collectionVariables.set(\"drone_token\", response.data.token);\r",
									"    console.log(\"✅ Drone token saved:\", response.data.token);\r",
									"} else {\r",
									"    console.warn(\"⚠️ No token found in Drone response\");\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"name\": \"Drone One\",\n  \"type\": \"drone\"\n}"
						},
						"url": {
							"raw": "http://127.0.0.1:8000/api/token",
							"protocol": "http",
							"host": [
								"127",
								"0",
								"0",
								"1"
							],
							"port": "8000",
							"path": [
